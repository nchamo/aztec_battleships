{
  "name": "battleships-dapp",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist src/target src/artifacts/*.json src/artifacts/*.js src/artifacts/*.d.ts contracts/*/target",
    "build-contracts": "npx tsx scripts/build-contracts.ts",
    "deploy-contracts": "tsx scripts/deploy.ts --network sandbox",
    "dev": "vite",
    "build-app": "node --max-old-space-size=14336 node_modules/vite/bin/vite.js build",
    "build": "yarn build-contracts && yarn build-app",
    "serve": "vite preview --port ${PORT:-3000}",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aztec/accounts": "4.0.0-nightly.20260122",
    "@aztec/aztec": "4.0.0-nightly.20260122",
    "@aztec/aztec.js": "4.0.0-nightly.20260122",
    "@aztec/constants": "4.0.0-nightly.20260122",
    "@aztec/foundation": "4.0.0-nightly.20260122",
    "@aztec/kv-store": "4.0.0-nightly.20260122",
    "@aztec/pxe": "4.0.0-nightly.20260122",
    "@aztec/stdlib": "4.0.0-nightly.20260122",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-tooltip": "1.2.8",
    "@tanstack/react-query": "^5.62.11",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "vite-plugin-node-polyfills": "0.22.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@aztec/test-wallet": "4.0.0-nightly.20260122",
    "@types/node": "^20.17.18",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^4.0.15"
  }
}
